{
  "outputDirectory": "frontend/build",
  "framework": null,
  "buildCommand": "",
  "installCommand": "",
  "rewrites": [
    {
      "source": "/api/chat",
      "destination": "/api/chat.py"
    },
    { "source": "/api/test", "destination": "/api/test.py" },
    { "source": "/api/auth/(.*)", "destination": "/api/index.py" },
    { "source": "/api/chats", "destination": "/api/index.py" },
    { "source": "/api/suggestions", "destination": "/api/index.py" },
    {
      "source": "/manifest.json",
      "destination": "/frontend/build/manifest.json"
    },
    {
      "source": "/favicon.ico",
      "destination": "/frontend/build/favicon.ico"
    },
    {
      "source": "/logo192.png",
      "destination": "/frontend/build/logo192.png"
    },
    {
      "source": "/logo512.png",
      "destination": "/frontend/build/logo512.png"
    },
    {
      "source": "/static/(.*)",
      "destination": "/frontend/build/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/build/index.html"
    }
  ],
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
